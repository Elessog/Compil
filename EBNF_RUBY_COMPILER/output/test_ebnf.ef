character = (letter) | (digit) | (symbol) |( "_" );

alphaNum = (letter )| (digit) | ("_") ;
identifier = letter , { alphaNum } ;
terminal = ("'" , character , { character } , "'" )
         | ('"' , character , { character } , '"' );
 
lhs = identifier ;
rhs = (identifier)
     | (terminal)
     |( "[" , rhs , "]")
     |( "{" , rhs , "}")
     |( "(" , rhs , ")")
     |( rhs , "|" , rhs)
     |( rhs , "," , rhs) ;
rule =  lhs , "=" , rhs , ";";
grammar = { rule  };
